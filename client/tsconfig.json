{
  "compilerOptions": {
    "module": "es2020",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "es2020",
    "jsx": "react",
    "strict": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "*": ["types/*"]
    }
  },
  "exclude": ["node_modules"]
}
